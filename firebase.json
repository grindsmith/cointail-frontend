{
  "firestore": {
    "rules": "firestore/firestore.rules",
    "indexes": "firestore/firestore.indexes.json"
  },
  "functions": [
    {
      "source": "function",
      "codebase": "default",
      "ignore": [
        "node_modules",
        ".git",
        "firebase-debug.log",
        "firebase-debug.*.log"
      ],
      "env": {
        "NODE_ENV": "production",
        "ALCHEMY_API_KEY": "4zCwn0JqW90GVky38PkaIaKYK-zQF1vu"
      }
    }
  ],
  "hosting": [
    {
      "target": "react",
      "source": "react",
      "ignore": [
        "firebase.json",
        "**/.*",
        "**/node_modules/**"
      ],
      "frameworksBackend": {
        "region": "us-central1"
      }
    },
    {
      "target": "nextjs",
      "source": "nextjs",
      "ignore": [
        "firebase.json",
        "**/.*",
        "**/node_modules/**"
      ],
      "frameworksBackend": {
        "region": "us-central1"
      }
    }
  ],
  "emulators": {
    "firestore": {
      "port": 8080
    },
    "hosting": {
      "port": 6001
    },
    "ui": {
      "enabled": true,
      "port": 5003
    },
    "singleProjectMode": true
  },
  "extensions": {
    "moralis-streams": "moralis/moralis-streams@0.1.16"
  }
}
